{
  "env": {
    "browser": {},
    "mocha": {},
    "node": {}
  },
  "ignore": [
    "third_party"
  ],
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "modules": "commonjs"
      }
    ],
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-transform-flow-strip-types",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-jsx"
  ]
}
